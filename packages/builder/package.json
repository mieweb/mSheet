{
  "name": "@msheet/builder",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@msheet/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build:css": "cd ../.. && npx @tailwindcss/cli -i packages/builder/src/index.css -o packages/builder/src/index.output.css",
    "build:css:watch": "cd ../.. && npx @tailwindcss/cli -i packages/builder/src/index.css -o packages/builder/src/index.output.css --watch"
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "tags": [
      "scope:builder"
    ]
  },
  "dependencies": {
    "@msheet/core": "^0.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tslib": "^2.3.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.2.1",
    "@tailwindcss/vite": "^4.2.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "jsdom": "^28.1.0",
    "tailwindcss": "^4.2.1"
  }
}
